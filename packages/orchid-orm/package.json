    "rollup-plugin-esbuild": "^4.10.1",
    "orchid-orm-schema-to-zod": "workspace:*",
    "@swc/core": "^1.2.210",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^28.1.2",
    "@types/node": "^18.0.1",
    "@types/pg": "^8.6.5",
    "dotenv": "^16.0.1",
    "jest": "^28.1.2",
    "pg": "^8.7.3",
    "pg-transactional-tests": "^1.0.5",
    "rimraf": "^3.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
