{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.75.0",
  "pnpmVersion": "7.4.1",
  "pnpmOptions": {
    "pnpmStore": "global",
    "strictPeerDependencies": false,
    "resolutionStrategy": "fewer-dependencies",
    "preventManualShrinkwrapChanges": false,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=12.13.0",
  "suppressNodeLtsWarning": true,
  "ensureConsistentVersions": false,
  "projectFolderMinDepth": 1,
  "projectFolderMaxDepth": 2,
  "allowMostlyStandardPackageNames": false,
  "repository": {
    // "url": "https://github.com/microsoft/rush-example",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [
      // "common/scripts/pre-rush-install.js"
    ],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "telemetryEnabled": false,
  "hotfixChangeEnabled": false,
  // "allowedProjectTags": [ "apps", "Web", "tools" ],
  "projects": [
    {
      "packageName": "orchid-orm",
      "projectFolder": "packages/orchid-orm",
      "shouldPublish": true,
      "tags": ["orm"]
    },
    {
      "packageName": "pqb",
      "projectFolder": "packages/pqb",
      "shouldPublish": true,
      "tags": ["query-builder"]
    },
    {
      "packageName": "rake-db",
      "projectFolder": "packages/rake-db",
      "shouldPublish": true,
      "tags": ["migrations"]
    },
    {
      "packageName": "orchid-orm-schema-to-zod",
      "projectFolder": "packages/schema-to-zod",
      "shouldPublish": true,
      "tags": ["schema", "zod"]
    },
    {
      "packageName": "orchid-orm-test-factory",
      "projectFolder": "packages/test-factory",
      "shouldPublish": true,
      "tags": ["test-factory"]
    }
  ]
}
